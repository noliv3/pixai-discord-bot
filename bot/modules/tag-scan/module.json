  "description": "Image tag scanning and NSFW classification",
  "commands": ["scanconfig", "setscan"],
  "events": ["messageCreate"],
  "configKeys": ["scan", "modules.tag-scan"],
  "defaultConfig": {
    "enabled": true,
    "thresholds": {
      "flag": 0.6,
      "delete": 0.95
    }
  }
}
